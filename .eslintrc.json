{
    "plugins": ["prettier", "import"],
    "extends": [
        "plugin:prettier/recommended",
        "prettier"
    ],
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 8
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "no-debugger": "off",
        "no-console": 0,
        "class-methods-use-this": "off",
        "prettier/prettier": ["warn",{
            "endOfLine": "auto"}
          ]
    }
}
